package com.webagesolutions.records;

public class VMFrameApp
{
  private Model model;
  private BeanRecord beanRecord;
  
  public VMFrameApp(Model model, BeanRecord beanRecord)
  {
    this.model = model;
    this.beanRecord = beanRecord;
  }
  
  public BeanRecord currentBeanRecord()
  {
    return beanRecord;
  }
}

package com.webagesolutions.threads.synch;

public class Producer
{
  private Stack stack;
  private static int counter = 1;
  private int num;
  
  public Producer(Stack stack)
  {
    this.stack = stack;
    num = counter++;
  }
  
  public String getName()
  {
    return "Producer " + this.num; 
  }
}

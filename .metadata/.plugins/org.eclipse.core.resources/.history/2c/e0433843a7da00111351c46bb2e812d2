package com.websolutions;

import com.webagesolutions.dom.DOMNodeCount;
import com.webagesolutions.sax.SAXNodeCount;

public class NodeCountRunner
{
  public static void main(String[] args)
  {
    new DOMNodeCount(args[0]);
    new SAXNodeCount(args[0]);
    new DOMNodeCount(args[1]);
    new SAXNodeCount(args[1]);
  }
}
